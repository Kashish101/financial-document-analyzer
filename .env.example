# Copy this file to .env and fill in your API keys

# Required: OpenAI API Key (get from https://platform.openai.com)
OPENAI_API_KEY=your_openai_api_key_here

# Optional: Choose model (default: gpt-4o-mini)
OPENAI_MODEL_NAME=gpt-4o-mini

# Optional: Serper API Key for web search tool (get from https://serper.dev)
SERPER_API_KEY=your_serper_api_key_here

# Optional: Redis URL for queue worker (bonus feature)
REDIS_URL=redis://localhost:6379/0
